<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity BillPayment.
    -->
    <changeSet id="20170915181800-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="bill_payment"
                                 constraintName="fk_bill_payment_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vendor"/>

        <addForeignKeyConstraint baseColumnNames="account_user_id"
                                 baseTableName="bill_payment"
                                 constraintName="fk_bill_payment_account_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="account"/>

    </changeSet>
</databaseChangeLog>
